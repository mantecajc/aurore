<%= form_for @song do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.label :link_youtube %><br>
    <%= f.text_field :youtube_url %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Cancel', songs_path %>